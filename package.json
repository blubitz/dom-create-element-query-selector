{
  "name": "makel-dom",
  "version": "1.0.0",
  "description": "A utility function to make DOM elements with CSS selector-like syntax",
  "main": "lib/index.js",
  "files": [
    "src",
    "lib"
  ],
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "build": "clear && npm run lint:test && babel src -d lib",
    "prepublish": "npm run build",
    "precommit": "npm run lint:test",
    "prepush": "npm run lint:test",
    "release": "release-it",
    "lint": "npx eslint src --ext .js",
    "test": "clear && jest",
    "test:coverage": "jest --coverage",
    "test:watch": "clear && jest --watch",
    "lint:test": "npm run lint && npm run test:coverage"
  },
  "keywords": [
    "dom",
    "create element",
    "makel",
    "html",
    "querySelector",
    "css"
  ],
  "author": "blubitz",
  "license": "MPL-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/blubitz/makel-dom.git"
  },
  "private": true,
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "cross-env": "^3.1.3",
    "eslint": "^7.18.0",
    "jest": "^26.6.3",
    "release-it": "^14.2.2",
    "rimraf": "^2.5.4"
  }
}
